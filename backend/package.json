{
   "type": "module",
   "devDependencies": {
      "@babel/cli": "^7.28.3",
      "@babel/core": "^7.28.5",
      "@babel/node": "^7.28.0",
      "@babel/preset-env": "^7.28.5",
      "@babel/preset-react": "^7.28.5",
      "@babel/register": "^7.28.3",
      "nodemon": "^3.1.11",
      "terser": "^5.44.1",
      "tsx": "^4.21.0"
   },
   "scripts": {
      "email": "email dev",
      "email:watch": "nodemon --watch src --ext jsx --exec \"npm run email\"",
      "dev": "set NODE_ENV=development&& nodemon src/server.js --watch src --exec tsx",
      "prod": "set NODE_ENV=prodlocale && node src/server.js",
      "preview:email": "tsx ../emails/preview-email.js",
      "build:emails": "tsx --transpile-only emails/src/Template-email-auto-response.jsx --out-dir build/emails",
      "copy-emails": "shx cp -r emails ../build/",
      "clean": "del /s /q ..\\build\\*.* && if exist ..\\build rmdir /s /q ..\\build",
      "build": "babel src --out-dir ../build/backend --extensions \".js,.jsx\" --minified",
      "start": "node ../build/backend/server.js"
   },
   "dependencies": {
      "@react-email/components": "^1.0.2",
      "@react-email/render": "^2.0.0",
      "cors": "^2.8.5",
      "dotenv": "^17.2.3",
      "express": "^5.2.1",
      "express-validator": "^7.3.1",
      "nodemailer": "^7.0.11",
      "sharp": "^0.34.5",
      "zod": "^4.3.6"
   }
}
